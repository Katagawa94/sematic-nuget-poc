{
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@droidsolutions-oss/semantic-release-nuget",
      {
        "packCommand": "dotnet pack --configuration Release --output ./packages",
        "pushCommand": "dotnet nuget push ./packages/*.nupkg --api-key ${GITHUB_TOKEN} --source https://nuget.pkg.github.com/@Katagawa94/index.json"
      }
    ],
    "@semantic-release/git",
    "@semantic-release/github"
  ]
}

